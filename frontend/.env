# React App Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0
REACT_APP_NAME=WIQ Frontend

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3

# Feature Flags
REACT_APP_FEATURE_USER_SWITCH=true
REACT_APP_FEATURE_SEARCH=true
REACT_APP_FEATURE_FILTERS=true
REACT_APP_FEATURE_COMMENTS=true
REACT_APP_FEATURE_STATS=true

# UI Configuration
REACT_APP_DEFAULT_USER_ROLE=admin
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_AUTO_SAVE_DELAY=1000

# Development Tools
REACT_APP_ENABLE_LOGGING=true
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_REDUX_DEVTOOLS=true

# Analytics (if implementing later)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_HOTJAR_ID=
REACT_APP_SENTRY_DSN=



# Performance
REACT_APP_DEBOUNCE_DELAY=300
REACT_APP_THROTTLE_DELAY=1000

